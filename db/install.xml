<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/generalvote/db" VERSION="2023100900" COMMENT="XMLDB file for Moodle generalvote block"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>

    <TABLE NAME="block_generalvote_votes" COMMENT="Contains votes for the generalvote block">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true"/>
        <FIELD NAME="vote" TYPE="text" LENGTH="small" NOTNULL="true" DEFAULT=""/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>

  </TABLES>
</XMLDB>